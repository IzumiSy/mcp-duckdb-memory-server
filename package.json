  "devDependencies": {
    "@types/node": "^22.13.5",
    "prettier": "^3.5.2",
    "shx": "^0.3.4",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
